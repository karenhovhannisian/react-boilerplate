{
  "name": "React-Boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "SET NODE_ENV=production && node node_modules/webpack/bin/webpack.js --config webpack.config.production.js --progress --p --display-error-details",
    "start": "NODE_ENV=production node server",
    "start:dev": "SET NODE_ENV=development && node node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --profile --colors",
    "compile": "rm -rf dist rm production/js/bundle.* && rm production/css/* && npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.35",
    "@types/jest": "^20.0.8",
    "@types/js-cookie": "^2.0.28",
    "@types/node": "^7.0.22",
    "@types/react": "15.0.36",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.47",
    "@types/react-router": "^3.0.13",
    "@types/reactstrap": "^4.3.13",
    "@types/redux-immutable": "^3.0.37",
    "@types/validator": "^6.2.3",
    "@types/webpack": "^2.2.5",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bootstrap-sass": "^3.3.7",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.27.3",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-babel-module": "^2.2.1",
    "eslint-import-resolver-babel-module-alias": "^1.5.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-dependencies": "^2.2.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "jest": "^21.1.0",
    "node-sass": "^4.5.0",
    "react-hot-loader": "^1.3.1",
    "redux-saga-test-plan": "^3.2.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.15.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.7.0",
    "webpack-cleanup-plugin": "^0.4.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "crypto-js": "^3.1.9-1",
    "express": "^4.15.5",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
