    "redux-logger": "^2.8.1",
    "redux-saga": "^0.15.4",
    "reselect": "^3.0.0",
    "validator": "^7.0.0"
  },
  "jest": {
    "testRegex": ".*\\.spec\\.js$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "assets"
    ]
  }
}
